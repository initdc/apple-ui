{
  "name": "apple-ui",
  "version": "0.0.0",
  "description": "An ui kit with many components looks like apple.com website.",
  "main": "dist/index.js",
  "scripts": {
    "build:css": "sh ./scripts/build-css.sh",
    "build:package": "sh ./scripts/build-package.sh",
