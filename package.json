    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/test-utils": "^1.1.4",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.13",
    "postcss-import": "^14.0.1",
    "postcss-nested": "^5.0.5",
    "rollup": "^2.45.2",
    "tailwindcss": "^2.1.2",
